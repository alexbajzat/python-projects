def isSorted(list):
   
    
    for i in range(0, len(list)-1):
        
            if list[i]>=list[i+1]:
                return False
        
    return True

def printList(list):
    
    for i in list:
        
            print(i,end=" ,") 
    
    
    


                
                
            
            
            
  
 
list=[1,2,3,7,5,6]


firstTime=True


while list!=[]:
    newList=[]
    
    for i in list:
        newList.append(i)
        
        if isSorted(list):
            printList(list)
            
        else:
            newList.pop()
    list.remove(list[0])
    
